  "scripts": {
    "test": "jest --coverage --config ./config/jest.config.json",
    "test:watch": "jest --coverage --config ./config/jest.config.json --watch",
    "build": "cross-env NODE_ENV=production webpack --progress --colors --config ./webpack.config.js",
    "start": "npm-run-all --parallel api front",
